{
  "name": "ask-the-world-api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "rimraf build && cross-env NODE_ENV=production webpack --config config/webpack.config",
    "start": "cross-env NODE_ENV=production node build/index.js",
    "start-local": "cross-env nodemon -L --exec ts-node -r tsconfig-paths/register src/index.ts",
    "prettier": "prettier --check src/**/*.ts",
    "lint": "eslint src/**/*.{ts,tsx}",
    "typecheck": "tsc --noEmit",
    "test": "cross-env NODE_ENV=test mocha -r ts-node/register -r tsconfig-paths/register src/**/*.spec.ts --timeout 10000 --exit",
    "test-int": "cross-env NODE_ENV=test mocha -r ts-node/register -r tsconfig-paths/register tests/index.ts --timeout 25000 --exit",
    "db-seed": "ts-node ./db/seed.ts",
    "db-clear": "ts-node ./db/clear.ts",
    "pull-shared": "git subtree pull --prefix src/ext/atw-shared https://$PAT@github.com/Karmello/ask-the-world-shared.git main --squash"
  },
  "dependencies": {
    "@honeybadger-io/js": "^6.1.0",
    "aws4": "^1.12.0",
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cross-env": "^7.0.3",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.15",
    "mongoose": "^7.4.1",
    "mongoose-unique-validator": "^4.0.0",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.2",
    "socket.io": "^4.6.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.22.4",
    "@babel/preset-typescript": "^7.22.5",
    "@babel/register": "^7.22.5",
    "@faker-js/faker": "^8.0.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.6",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash": "^4.14.195",
    "@types/mocha": "^10.0.1",
    "@types/mongoose-unique-validator": "^1.0.6",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.4.4",
    "@types/nodemailer": "^6.4.8",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "babel-loader": "^9.1.2",
    "babel-plugin-module-resolver": "^5.0.0",
    "chai": "^4.3.7",
    "chai-http": "^4.4.0",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.45.0",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.22",
    "prettier": "^3.0.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tslint": "^6.1.3",
    "typescript": "^5.1.3",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.1"
  },
  "resolutions": {
    "engine.io": ">=6.4.2",
    "got": ">=11.8.5"
  }
}
