{
  "name": "ask-the-world-api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "rimraf build && cross-env NODE_ENV=production webpack --config config/webpack.config",
    "start": "cross-env NODE_ENV=production node build/index.js",
    "start-local": "cross-env nodemon -L --exec ts-node -r tsconfig-paths/register src/index.ts",
    "prettier": "prettier --check src/**/*.ts",
    "lint": "eslint src/**/*.{ts,tsx}",
    "typecheck": "tsc --noEmit",
    "test": "cross-env NODE_ENV=test mocha -r ts-node/register -r tsconfig-paths/register src/**/*.spec.ts --timeout 10000 --exit",
    "test-int": "cross-env NODE_ENV=test mocha -r ts-node/register -r tsconfig-paths/register tests/index.ts --timeout 25000 --exit",
    "db-seed": "ts-node ./db/seed.ts",
    "db-clear": "ts-node ./db/clear.ts",
    "pull-shared": "git subtree pull --prefix src/ext/atw-shared https://$PAT@github.com/Karmello/ask-the-world-shared.git main --squash"
  },
  "dependencies": {
    "@honeybadger-io/js": "^5.4.1",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cross-env": "^7.0.3",
    "date-fns": "^2.23.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "helmet": "^4.2.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mongoose": "^6.4.3",
    "mongoose-unique-validator": "^3.1.0",
    "morgan": "^1.10.0",
    "nodemailer": "^6.5.0",
    "socket.io": "^4.3.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.21.5",
    "@babel/register": "^7.21.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.6",
    "@types/faker": "^4.1.11",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.194",
    "@types/mocha": "^10.0.1",
    "@types/mongoose-unique-validator": "^1.0.6",
    "@types/morgan": "^1.9.1",
    "@types/node": "^20.2.1",
    "@types/nodemailer": "^6.4.0",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "babel-loader": "^9.1.2",
    "babel-plugin-module-resolver": "^5.0.0",
    "chai": "^4.3.7",
    "chai-http": "^4.3.0",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.19.0",
    "faker": "^4.1.0",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.18",
    "prettier": "^2.8.8",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tslint": "^6.1.3",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  }
}
